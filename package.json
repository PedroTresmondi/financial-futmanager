{
  "name": "futebol-carteira",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"node server.js\"",
    "build": "vite build && node scripts/copy-to-dist.js",
    "preview": "vite preview",
    "server": "node server.js",
    "lint": "eslint src server.js scripts",
    "format": "prettier --write \"src/**/*.js\" \"server.js\" \"scripts/**/*.js\""
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "vite": "^7.3.1"
  }
}
